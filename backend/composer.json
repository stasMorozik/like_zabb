{
    "name": "stas/youdo-api-php",
    "autoload": {
        "psr-4": {
            "Core\\": "src/Core",
            "MySQLAdapters\\": "src/MySQLAdapters",
            "SMTPAdapters\\": "src/SMTPAdapters",
            "RestApi\\": "src/RestApi"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Core\\ConfirmationCode\\Adapters\\": "tests/Core/ConfirmationCode/Adapters",
            "Tests\\Core\\Account\\Adapters\\": "tests/Core/Account/Adapters",
            "Tests\\Core\\Role\\Adapters\\": "tests/Core/Role/Adapters",
            "Tests\\MySQLAdapters\\": "tests/MySQLAdapters"
        }
    },
    "authors": [
        {
            "name": "stasMorozik",
            "email": "stanim857@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "ramsey/uuid": "^4.5",
        "symfony/cache": "^5.4",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "symfony/console": "6.1.*",
        "symfony/dotenv": "6.1.*",
        "symfony/flex": "^2",
        "symfony/framework-bundle": "6.1.*",
        "symfony/runtime": "6.1.*",
        "symfony/yaml": "6.1.*",
        "sergeytsalkov/meekrodb": "^2.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "symfony/browser-kit": "6.1.*",
        "symfony/css-selector": "6.1.*",
        "symfony/phpunit-bridge": "^6.1"
    },
    "config": {
        "allow-plugins": {
            "symfony/flex": true,
            "symfony/runtime": true
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    }
}
